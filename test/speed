for foo in `cat spd-in.$$`
do
  ./idsgrep -I --statistics "$foo" chise.eids kanjivg.eids \
    >> spd-outa.$$
done
grep STAT spd-outa.$$ | \
  perl -e 'while(<>){@a=split;for($i=0;$i<=$#a;$i++){$b[$i]+=$a[$i];}}' \
  -e 'print join(" ","TOTAL",@b)."\n";'

for foo in `cat spd-in.$$`
do
  ./idsgrep --statistics "$foo" chise.eids kanjivg.eids \
    >> spd-outb.$$
done
grep STAT spd-outb.$$ | \
  perl -e 'while(<>){@a=split;for($i=0;$i<=$#a;$i++){$b[$i]+=$a[$i];}}' \
  -e 'print join(" ","TOTAL",@b)."\n";'

grep -v STAT spd-outa.$$ > spd-outc.$$
grep -v STAT spd-outb.$$ > spd-outd.$$

if diff -c spd-outc.$$ spd-outd.$$
then
  rm -f spd-in.$$ spd-out[abcd].$$
  exit 0
else
  rm -f spd-in.$$ spd-out[abcd].$$
  exit 1
fi
